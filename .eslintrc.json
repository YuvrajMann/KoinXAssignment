// This file is used to configure the ESLint linter.
// For a list of available rules, see https://eslint.org/docs/rules/

{
    "env": {
        "browser": true,
        "es2021": true,
        "node": true,
        "es6": true
    },
    "extends": ["eslint:recommended","prettier", "plugin:react/recommended"],
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": ["react"],
    "rules": {
        "no-console":2,                   
        "comma-dangle": 1,               
        "quotes":2,
        "Linebreak-style":0,      
        "no-unused-vars": 0,             
        "max-len": 0
    }
}